{
  "extends": "stylelint-config-recommended",
  "plugins": [
    "stylelint-no-unsupported-browser-features"
  ],
  "rules": {
    "plugin/no-unsupported-browser-features": [true, {
      "severity": "warning"
    }],
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "import-normalize",
        "custom-selectors",
      ]
    }],
    "unit-whitelist": ["em", "rem", "ms", "s", "%", "vh", "vw"]
  }
}
