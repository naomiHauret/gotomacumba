    "fela-plugin-prefixer": "^5.0.19",
    "fela-tools": "^5.2.0",
    "hygen": "^1.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0"
  },
  "author": "Naomi Hauret",
  "license": "MIT"
}
